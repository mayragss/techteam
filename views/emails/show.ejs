<div class="container">
    <div class="row">
        <div class="col-md-3">
            <p class="lead">Email</p>
            
        </div>
        <div class="col-md-9">
            <div class="thumbnail">
                <img class="img-responsive" src="<%= email.image %>">
                <div class="caption-full">
                    <h4><a><%=email.email%></a></h4>
                    <p><%= email.message %></p>
                    <p>
                        <em>Enviado por <%= email.nome %></em>
                    </p>
                    
                </div>
            </div>
            
        </div>
    </div>
</div>
